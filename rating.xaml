<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="WpfApp1.rating"
        mc:Ignorable="d"
        Title="" Height="300" Width="200" Background="#FF97E2DB">
    <Grid>
        <Button x:Name="submitRate" Content="Submit" HorizontalAlignment="Center" Height="25" Margin="0,142,0,0" VerticalAlignment="Top" FontSize="12" Width="76" Click="submitRate_Click"/>
        <materialDesign:RatingBar x:Name="rateBar" HorizontalAlignment="Center" Height="27" Margin="0,90,0,0" VerticalAlignment="Top" Width="124" Foreground="#FFDAA841" BorderBrush="Black" Background="#FF4055B4"/>

    </Grid>
</Window>
